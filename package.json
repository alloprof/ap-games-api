{
  "name": "ap-api-boilerplate",
  "version": "0.0.1",
  "description": "Alloprof API Boilerplate",
  "main": "index.js",
  "engines": {
    "node": ">=20.14.0",
    "npm": ">=10.7.0"
  },
  "scripts": {
    "dev": "tsc -w & node --watch dist/index.js | bunyan",
    "test": "jest --watchAll",
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc",
    "build:production": "npm run clean && tsc && cp package.json package-lock.json dist/ && cd dist && npm install --only=production && cd ..",
    "coverage": "jest --coverage"
  },
  "author": "Alloprof",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/logging-bunyan": "^5.1.0",
    "bunyan": "^1.8.15",
    "dotenv": "^16.4.5",
    "express": "^4.21.1"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.11",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "superagent": "^9.0.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.5",
    "typescript": "^5.4.5"
  }
}
